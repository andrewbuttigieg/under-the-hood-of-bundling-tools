    "fs-promise": "^1.0.0",
    "lodash": "^4.17.4",
    "module-deps": "^4.0.8",
    "node-browserify": "^1.0.0",
    "rollup": "^0.41.4",
    "tape": "^4.6.3",
    "webpack": "^2.2.1",
    "yargs": "^6.6.0"
